{
    "name": "labstag/labstag",
    "description": "Un site Internet sous Symfony",
    "type": "labstag/labstag",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.2",
        "api-platform/core": "^2.3",
        "beberlei/doctrineextensions": "^1.2",
        "cocur/slugify": "^3.2",
        "csa/guzzle-bundle": "^3.1",
        "dompdf/dompdf": "^0.8.3",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "giggsey/libphonenumber-for-php": "^8.10",
        "imagine/imagine": "^1.2",
        "knplabs/knp-components": "^1.3",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^3.0",
        "knplabs/knp-time-bundle": "^1.9",
        "knpuniversity/oauth2-client-bundle": "^1.28",
        "league/glide-symfony": "^1.0",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-github": "^2.0",
        "league/oauth2-google": "^3.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "martinssipenko/password-policy": "^1.0",
        "phpoffice/phppresentation": "^0.9.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "phpoffice/phpword": "^0.16.0",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-console": "^1.1",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "sentry/sentry-symfony": "^2.3",
        "setasign/fpdi-fpdf": "^2.2",
        "stevenmaguire/oauth2-bitbucket": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.2.*",
        "symfony/browser-kit": "4.2.*",
        "symfony/config": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/css-selector": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/intl": "4.2.*",
        "symfony/ldap": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.2.*",
        "symfony/security": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "4.2.*",
        "symfony/thanks": "^1.1",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/var-dumper": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/workflow": "4.2.*",
        "symfony/yaml": "4.2.*",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.9",
        "webonyx/graphql-php": "^0.13.0",
        "wohali/oauth2-discord-new": "^1.0",
        "zendframework/zend-soap": "^2.7"
    },
    "require-dev": {
        "codeception/codeception": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.8",
        "pdepend/pdepend": "^2.5",
        "phan/phan": "^1.2",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.15.0",
        "phpstan/phpstan-symfony": "^0.11.3",
        "povils/phpmnd": "^2.1",
        "sebastian/phpcpd": "^4.1",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^0.3.0",
        "symfony/phpunit-bridge": "4.2.*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "phploc": "phploc src",
        "php-cs-fixer": "php-cs-fixer fix --config=php_cs.dist src -n",
        "pdepend": "pdepend --summary-xml=summary.xml --jdepend-chart=jdepend.svg --overview-pyramid=pyramid.svg src/",
        "phpcbf": "phpcbf src tests --ignore=*/src/Migrations/*,src/Kernel.php",
        "phpcs": "phpcs src --ignore=*/src/Migrations/*,src/Kernel.php",
        "phpmnd": [
            "phpmnd src",
            "phpmnd tests"
        ],
        "phpmd": "phpmd src,tests text phpmd.xml",
        "phpcpd": "phpcpd src",
        "phpunit": "phpunit tests",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
