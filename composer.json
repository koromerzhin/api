{
    "name": "labstag/core",
    "description": "Un site Internet sous Symfony",
    "type": "projects",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-xml": "*",
        "api-platform/api-pack": "1.2.1",
        "api-platform/core": "2.5.3",
        "beberlei/doctrineextensions": "1.2.6",
        "cocur/slugify": "4.0.0",
        "csa/guzzle-bundle": "3.1.1",
        "doctrine/doctrine-migrations-bundle": "2.1",
        "dompdf/dompdf": "0.8.3",
        "giggsey/libphonenumber-for-php": "8.11.1",
        "imagine/imagine": "1.2.3",
        "knplabs/knp-components": "2.3.0",
        "knplabs/knp-menu-bundle": "3.0.0",
        "knplabs/knp-paginator-bundle": "5.0.0",
        "knplabs/knp-time-bundle": "1.10.0",
        "lexik/jwt-authentication-bundle": "2.6.5",
        "martinssipenko/password-policy": "1.0.0",
        "phpoffice/phppresentation": "0.9.0",
        "phpoffice/phpspreadsheet": "1.10.1",
        "phpoffice/phpword": "0.17.0",
        "ramsey/uuid": "3.9.1",
        "ramsey/uuid-console": "1.1.2",
        "ramsey/uuid-doctrine": "1.5.0",
        "scienta/doctrine-json-functions": "4.1.1",
        "sensio/framework-extra-bundle": "5.5.2",
        "setasign/fpdi-fpdf": "2.2.0",
        "stevenmaguire/oauth2-dropbox": "3.0.1",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "4.4.1",
        "symfony/config": "4.4.1",
        "symfony/console": "4.4.1",
        "symfony/dependency-injection": "4.4.1",
        "symfony/dotenv": "4.4.1",
        "symfony/expression-language": "4.4.1",
        "symfony/flex": "1.6.0",
        "symfony/form": "4.4.1",
        "symfony/framework-bundle": "4.4.1",
        "symfony/intl": "4.4.1",
        "symfony/ldap": "4.4.1",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/orm-pack": "1.0",
        "symfony/process": "4.4.1",
        "symfony/security": "4.4.1",
        "symfony/security-bundle": "4.4.1",
        "symfony/serializer-pack": "1.0",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/templating": "4.4.1",
        "symfony/translation": "4.4.1",
        "symfony/twig-bundle": "4.4.1",
        "symfony/validator": "4.4.1",
        "symfony/var-dumper": "4.4.1",
        "symfony/web-link": "4.4.1",
        "symfony/workflow": "4.4.1",
        "symfony/yaml": "4.4.1",
        "twig/extensions": "1.5.4",
        "vich/uploader-bundle": "1.12.1",
        "webonyx/graphql-php": "0.13.8",
        "zendframework/zend-soap": "2.8.0"
    },
    "require-dev": {
        "codeception/codeception": "3.1.2",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "friendsofphp/php-cs-fixer": "2.16.1",
        "friendsoftwig/twigcs": "3.2.2",
        "fzaninotto/faker": "1.9.1",
        "maglnet/composer-require-checker": "2.0.0",
        "pdepend/pdepend": "2.6.0",
        "phan/phan": "2.4.5",
        "phploc/phploc": "5.0.0",
        "phpmd/phpmd": "2.7.0",
        "phpro/grumphp": "0.17.0",
        "phpstan/phpstan": "0.12.3",
        "povils/phpmnd": "2.1.0",
        "sebastian/phpcpd": "4.1.0",
        "sensiolabs/security-checker": "6.0.3",
        "squizlabs/php_codesniffer": "3.5.3",
        "symfony/browser-kit": "4.4.1",
        "symfony/css-selector": "4.4.1",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "1.14.3",
        "symfony/panther": "0.6.0",
        "symfony/phpunit-bridge": "4.4.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "1.0.6",
        "symfony/thanks": "1.2.4",
        "symfony/web-server-bundle": "4.4.1",
        "symfony/webpack-encore-bundle": "1.7.2"
    },
    "config": {
        "process-timeout": 0,
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Labstag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labstag\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "twigcs": "twigcs templates",
        "phploc": "phploc src",
        "fixphp": [
            "php-cs-fixer fix --config=php_cs.dist src  tests spec -n",
            "phpcbf src tests spec --ignore=src/Migrations/*,src/Kernel.php"
        ],
        "php-cs-fixer": "php-cs-fixer fix --config=php_cs.dist src  tests spec -n",
        "pdepend": "pdepend --summary-xml=summary.xml --jdepend-chart=jdepend.svg --overview-pyramid=pyramid.svg src/",
        "phpcbf": "phpcbf src tests spec --ignore=src/Migrations/*,src/Kernel.php",
        "phpcs": "phpcs src tests spec --ignore=src/Migrations/*,src/Kernel.php",
        "phpstan": "phpstan analyse src",
        "phpmnd": [
            "phpmnd src",
            "phpmnd tests",
            "phpmnd spec"
        ],
        "tests": [
            "phpunit",
            "phpspec run"
        ],
        "phpspec": "phpspec run",
        "phpmd": "phpmd src,tests,spec text phpmd.xml",
        "phpcpd": "phpcpd src tests spec",
        "phpunit": "phpunit",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
