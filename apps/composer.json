{
    "name": "labstag/core",
    "description": "Un site Internet sous Symfony",
    "type": "projects",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-xml": "*",
        "api-platform/api-pack": "1.2.1",
        "api-platform/core": "2.5.4",
        "beberlei/doctrineextensions": "1.2.6",
        "cocur/slugify": "4.0.0",
        "doctrine/doctrine-migrations-bundle": "2.1.2",
        "dompdf/dompdf": "0.8.5",
        "giggsey/libphonenumber-for-php": "8.11.4",
        "imagine/imagine": "1.2.3",
        "knplabs/knp-components": "2.3.3",
        "knplabs/knp-menu-bundle": "3.0.0",
        "knplabs/knp-paginator-bundle": "5.1.1",
        "knplabs/knp-time-bundle": "1.11.0",
        "laminas/laminas-soap": "2.8.0",
        "lexik/jwt-authentication-bundle": "2.6.5",
        "martinssipenko/password-policy": "1.0.0",
        "phpoffice/phppresentation": "0.9.0",
        "phpoffice/phpspreadsheet": "1.10.1",
        "phpoffice/phpword": "0.17.0",
        "ramsey/uuid": "3.9.3",
        "ramsey/uuid-console": "1.1.2",
        "ramsey/uuid-doctrine": "1.6.0",
        "scienta/doctrine-json-functions": "4.1.2",
        "sensio/framework-extra-bundle": "5.5.3",
        "sensiolabs/security-checker": "6.0.3",
        "setasign/fpdi-fpdf": "2.3.0",
        "stevenmaguire/oauth2-dropbox": "3.0.1",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "4.4.11",
        "symfony/config": "4.4.11",
        "symfony/console": "4.4.11",
        "symfony/dependency-injection": "4.4.11",
        "symfony/dotenv": "4.4.11",
        "symfony/expression-language": "4.4.11",
        "symfony/flex": "1.9.1",
        "symfony/form": "4.4.11",
        "symfony/framework-bundle": "4.4.11",
        "symfony/intl": "4.4.11",
        "symfony/ldap": "4.4.11",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/orm-pack": "1.1.0",
        "symfony/process": "4.4.11",
        "symfony/security": "4.4.11",
        "symfony/security-bundle": "4.4.11",
        "symfony/serializer-pack": "1.0.3",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/templating": "4.4.11",
        "symfony/translation": "4.4.11",
        "symfony/twig-bundle": "4.4.11",
        "symfony/validator": "4.4.11",
        "symfony/var-dumper": "4.4.11",
        "symfony/web-link": "4.4.11",
        "symfony/workflow": "4.4.11",
        "symfony/yaml": "4.4.11",
        "twig/extensions": "1.5.4",
        "vich/uploader-bundle": "1.13.1",
        "webonyx/graphql-php": "0.13.8"
    },
    "require-dev": {
        "bheller/images-generator": "1.0.1",
        "codeception/codeception": "4.1.1",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "fzaninotto/faker": "1.9.1",
        "maglnet/composer-require-checker": "2.0.0",
        "phpstan/extension-installer": "1.0.3",
        "squizlabs/php_codesniffer": "3.5.4",
        "symfony/browser-kit": "4.4.11",
        "symfony/css-selector": "4.4.11",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "1.20.0",
        "symfony/panther": "0.7.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "1.0.6",
        "symfony/thanks": "1.2.9",
        "symfony/web-server-bundle": "4.4.11"
    },
    "config": {
        "process-timeout": 0,
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Labstag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labstag\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.4"
        }
    }
}
